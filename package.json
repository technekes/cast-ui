{
  "name": "@tkxs/cast-ui",
  "version": "0.1.19",
  "author": "@tkxs",
  "description": "React component library for the TKXS design system",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/technekes/cast-ui"
  },
  "scripts": {
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006 --ci -c .storybook",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "clean": "shx rm -rf lib-cjs lib-esm lib-umd lib-es2015",
    "prettier": "pretty-quick --staged",
    "pre-commit": "npm run lint:fix && npm run prettier",
    "deploy-storybook": "storybook-to-ghpages --ci",
    "build": "npm run clean && npm run build:es2015 && npm run build:esm && npm run build:cjs && npm run build:umd && npm run build:umd:gzip",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir lib-es2015",
    "build:esm": "tsc --module es2015 --target es5 --outDir lib-esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir lib-cjs",
    "build:umd": "rollup -c rollup.config.prod.js",
    "build:umd:gzip": "cd lib-umd && gzip index.js -c > index.min.js.gz",
    "build-and-publish": "npm run build && npm publish",
    "build-dev": "npm run clean && tsc -w -m es6 --outDir lib-esm"
  },
  "files": [
    "lib-cjs/*",
    "lib-esm/*",
    "lib-es2015/*",
    "lib-umd/*"
  ],
  "browser": "lib-umd/index.js",
  "main": "lib-esm/index.js",
  "module": "lib-cjs/index.js",
  "types": "lib-esm/index.d.ts",
  "sideEffects": false,
  "peerDependencies": {
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "styled-components": "^4.1.0"
  },
  "dependencies": {
    "@tippy.js/react": "^2.1.1",
    "react-day-picker": "^7.2.4",
    "react-icons-kit": "^1.2.1",
    "react-modal": "^3.8.1",
    "react-paginate": "^6.2.1",
    "react-select": "^2.2.0",
    "react-table": "^6.8.6",
    "react-tabs": "^3.0.0",
    "tinycolor2": "^1.4.1",
    "@types/react-table": "^6.7.21",
    "@types/react-paginate": "^6.2.1",
    "@types/react-select": "^2.0.11",
    "@types/react-tabs": "^2.3.0",
    "@types/react-modal": "^3.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@storybook/addon-actions": "^5.0.0",
    "@storybook/addon-info": "^5.0.0",
    "@storybook/addon-knobs": "^5.0.0",
    "@storybook/addon-viewport": "^5.0.0",
    "@storybook/addons": "^5.0.0",
    "@storybook/react": "^5.0.0",
    "@storybook/storybook-deployer": "^2.7.0",
    "@types/lodash": "^4.14.118",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-slick": "^0.23.2",
    "@types/storybook-addon-jsx": "^5.4.0",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^4.1.0",
    "@types/storybook__addon-knobs": "^4.0.1",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.0.3",
    "@types/tinycolor2": "^1.4.1",
    "autoprefixer": "^9.5.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.11",
    "prettier": "1.16.4",
    "pretty-quick": "^1.10.0",
    "react": "^16.8.2",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-dom": "^16.8.2",
    "react-router-dom": "^4.3.1",
    "rollup": "^1.7.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.3",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.20.1",
    "shx": "^0.3.2",
    "storybook-addon-jsx": "^7.1.0",
    "styled-components": "^4.1.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tsutils": "^3.5.0",
    "typeface-roboto": "0.0.54",
    "typescript": "^3.2.2",
    "uglify-es": "^3.3.9",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.4"
  }
}
